@model Size


<div class="card">

    <div class="card-body">
        <h5 class="card-title"><b> Name:</b> @Model.Name</h5>
        <p class="card-text"><b>Product Count:</b> @Model.ProductSizes.Count</p>
        <p class="card-text"><b>Id:</b> @Model.Id</p>
       

        <a asp-controller="Size" asp-action="index" class="btn btn-success">Cancel</a>
    </div>
</div>


